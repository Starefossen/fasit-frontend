{
  "name": "fasit-frontend",
  "version": "297.0.0",
  "description": "Fasit frontend",
  "main": "server.js",
  "scripts": {
    "build-watch": "parcel watch src/index.html",
    "start-watch": "NODE_ENV=standalone PORT=6969  FASIT_URL=http://localhost:6969/mockapi node server.js",
    "dev": "concurrently --kill-others \"npm run start-watch\" \"npm run build-watch\"",
